{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "*.json"
    ]
  },
  "rules": {
    "indent": [true, "spaces", 4],
    "ordered-imports": [ true, { "import-sources-order": "any", "named-imports-order": "any"}],
    "no-console": false
  }
}
